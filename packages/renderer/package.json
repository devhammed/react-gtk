{
  "name": "react-gtk-renderer",
  "version": "1.0.2",
  "description": "React GTK+ Renderer",
  "main": "dist/index.js",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "prepublishOnly": "npm run clean && npm run build",
    "build": "npx parcel build src/index.js --no-source-maps",
    "dev": "npx parcel watch src/index.js --no-source-maps --no-hmr",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devhammed/react-gtk.git"
  },
  "author": "Hammed Oyedele",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/devhammed/react-gtk/issues"
  },
  "homepage": "https://github.com/devhammed/react-gtk#readme",
  "devDependencies": {
    "parcel": "^2.8.3"
  },
  "dependencies": {
    "react-reconciler": "^0.29.0"
  }
}
